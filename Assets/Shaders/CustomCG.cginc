//////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
inline fixed3 ToDigital(fixed3 analog255)
{
	return round(clamp(analog255, 0, 255));
}

//////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
inline float RGBToDepth (fixed3 digitalRGB)
{
	uint r = digitalRGB.r;
	uint g = digitalRGB.g;
	if (r % 2 == 1)
		g = 255 - g;
	uint intDepth = 256 * r + g;
	float depth = intDepth / 4000.0;
	return depth;
}